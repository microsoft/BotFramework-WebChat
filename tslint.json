{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": false,
    "max-line-length": false,
    "max-classes-per-file": false,
    "member-ordering": false,
    "member-access": false,
    "no-shadowed-variable": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "jsx-double",
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
