{
  "name": "botframework-directlinespeech-sdk",
  "version": "0.0.0-0",
  "description": "Direct Line Speech SDK",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "scripts": {
    "eslint": "eslint src/**/*.js --ignore-pattern *.spec.js --ignore-pattern *.test.js --ignore-pattern __tests__",
    "prepublishOnly": "npm run prepublishOnly:babel && npm run prepublishOnly:webpack",
    "prepublishOnly:babel": "babel src --ignore **/*.spec.js,**/*.test.js,__tests__/**/*.js --out-dir lib --verbose",
    "prepublishOnly:webpack": "concurrently --names \"dev,prod\" \"npm run prepublishOnly:webpack:development\" \"npm run prepublishOnly:webpack:production\"",
    "prepublishOnly:webpack:development": "cross-env node_env=development webpack-cli",
    "prepublishOnly:webpack:production": "cross-env node_env=production webpack-cli",
    "prestart": "npm run prepublishOnly:babel",
    "start": "npm run start:babel && concurrently --kill-others --names \"babel,serve,webpack\" \"npm run start:babel:watch\" \"npm run start:serve\" \"npm run start:webpack\"",
    "start:babel": "npm run prepublishOnly:babel --",
    "start:babel:watch": "npm run start:babel -- --skip-initial-build --watch",
    "start:serve": "serve",
    "start:webpack": "npm run prepublishOnly:webpack:development -- --watch",
    "test": "jest --watch",
    "test:ci": "npm run test -- --ci --coverage true --no-watch"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "global-agent": "^2.1.7",
    "has-resolved": "^1.1.0",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "serve": "10.1.2",
    "simple-update-in": "^2.1.1",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-stats-plugin": "^0.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "abort-controller": "^3.0.0",
    "abort-controller-es5": "1.0.1",
    "base64-arraybuffer": "^0.2.0",
    "core-js": "^3.5.0",
    "event-as-promise": "^1.0.5",
    "event-target-shim": "^5.0.1",
    "event-target-shim-es5": "1.0.1",
    "math-random": "^1.0.4",
    "microsoft-cognitiveservices-speech-sdk": "^1.8.1",
    "p-defer": "^3.0.0",
    "p-defer-es5": "1.0.1",
    "web-speech-cognitive-services": "^6.0.0"
  }
}
