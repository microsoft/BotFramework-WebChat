{
  "name": "mockbot",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "build": "npx concurrently --names \"babel,typecheck\" \"npm run build:babel\" \"npm run build:typecheck\"",
    "build:babel": "babel --extensions .js,.ts,.tsx --ignore **/*.spec.js,**/*.spec.ts,**/*.spec.tsx,**/*.test.js,**/*.test.ts,**/*.test.tsx,__test__/**/*.js,__test__/**/*.ts,__test__/**/*.tsx --out-dir lib --source-maps inline --verbose src",
    "build:typecheck": "tsc",
    "clean": "npx rimraf lib",
    "start": "npx node-dev .",
    "watch": "npx concurrently --names \"babel,typecheck\" \"npm run build:babel -- --watch\" \"npm run build:typecheck -- --preserveWatchOutput --watch\""
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)(spec|test).[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@types/node": "^10.5.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "botbuilder": "4.0.0-preview1.2",
    "dotenv": "^6.0.0",
    "restify": "^7.2.1",
    "serve-handler": "^3.6.1"
  }
}
