  "scripts": {
    "build": "npm run build:babel && npm run build:esbuild",
    "build:babel": "babel --out-dir lib/ src/",
    "build:esbuild": "esbuild lib/index.js --bundle --outfile=dist/test-page-object.js --sourcemap --target=chrome80",
