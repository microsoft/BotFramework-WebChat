  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "concurrently": "^6.3.0",
    "esbuild": "^0.12.29",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
