  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run --if-present build:pre && npm run build:run && npm run --if-present build:post",
    "build:pre": "npm run build:pre:local-dependencies",
    "build:pre:local-dependencies": "../../../scripts/npm/build-local-dependencies.sh",
