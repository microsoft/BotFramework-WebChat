    "bump": "vg bump prod && vg bump dev && (npm audit fix || exit 0)",
    "postversion": "../../../scripts/npm/postversion.sh",
    "preversion": "../../../scripts/npm/preversion.sh",
    "start": "node ."
  },
  "pinDependencies": {},
  "localDependencies": {},
